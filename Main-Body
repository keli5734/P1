                        kappa_a,mu,rho,epsilon,lambda);
         
          q1_max_time(j,1) = q1(j);
          [maxValue, indexOfMaxValue] = max(y_inf_q1(:,2)); % betaValue
          q1_max_time(j,2) = maxValue; % MaxValue
          q1_max_time(j,3) = (indexOfMaxValue + 1) /24; % days
          
 end 

%%

figure(8)
scatter(q1_max_time(:,3), q1_max_time(:,2),'o',...
              'MarkerFaceColor',[0.3010 0.4450 0.9330],...
              'LineWidth',.8, 'MarkerEdgeAlpha',.5, 'MarkerFaceAlpha',.5, 'MarkerEdgeColor', 'none')

xlabel('The day M1 peaks') 
ylabel('Peak values of M1')
set(gca, 'FontSize',24)


















%% sensitivity to viral infectivity 'k_m1'
kappa = 7.7e-7:2e-6:7.7e-5;
len_kappa = length(kappa);
plot_var_k = zeros(len_kappa,2);
T_var_k = zeros(len_kappa,1);
for j = 1:len_kappa

        [~,y_inf_k] = ode15s(@fun_inf_lambda, report_time_inf, init_inf, options,...
                        k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,...
                        beta,delta_I,p,c,kappa(j),q1,q2,...
                        kappa_a,mu,rho,epsilon,lambda);
                    
         plot_var_k(j,1) = trapz(y_inf_k(:,6));
         plot_var_k(j,2) = trapz(y_inf_k(:,2));   
         T_var_k(j) = y_inf_k(end,4);
         %{
         figure(j)
         yyaxis left 
         semilogy(report_time_inf, y_inf_q2(:,6),'LineWidth',1.2)
         yyaxis right 
         plot(report_time_inf, y_inf_q2(:,2),'k','LineWidth',1.2)
         hold on
         plot(report_time_inf, y_inf_q2(:,3),'r','LineWidth',1.2)
         %}
end



%% Plot_k_m1_sensitivity
figure(36)
yyaxis left 
semilogx(kappa,plot_var_k(:,2),'k-.','LineWidth',2)
ylabel('AUC of M1')
set(gca, 'ycolor','k')
yyaxis right 
semilogx(kappa,plot_var_k(:,1),'k-','LineWidth',2)
ylabel('AUC of viral Load')
set(gca, 'ycolor','k')
legend('M1','V')
xlabel('log(q_1)') 

set(gca, 'FontSize',24)

%%
figure(30)
semilogx(kappa, T_var_k, 'k-', 'LineWidth', 2)

xlabel('k_{-1}') 
ylabel('Number of epithelial cells')
set(gca, 'FontSize',24)

