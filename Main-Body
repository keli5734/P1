%% M1-M-M2
clear
clc

%% Parameters 

k1 = 0.5;
k2 = 0.5;  
k_m1 = 24/72; %24/72; %24/72;
k_m2 = 24/72;
delta = 0.02;
s1 = 1;
s2 = 1;
g = .2;
epsilon = 0;

%% time setting 

t0 = 0;
t_end = 1000;
report_point = (t_end - t0) * 24 + 1;
report_time = linspace(t0,t_end,report_point);
len = length(report_time);

%% Variables 

M = zeros(1,len);
M1 = zeros(1,len);
M2 = zeros(1,len);

M1_1 = zeros(1,len);
M1_2 = zeros(1,len);
M1_3 = zeros(1,len);



M0 = 1e+6;
M10 = 0;
M20 = 0;

M1_10 = 0;
M1_20 = 0;
M1_30 = 0;

M(1) = M0;
M1(1) = M10;
M2(1) = M20;
M1_1(1) = M1_10;
M1_2(1) = M1_20;
M1_3(1) = M1_30;

init = [M0,M10,M20,M1_10,M1_20,M1_30]';

options = odeset('RelTol',1e-10,'AbsTol',1e-20);

%%

[~,y] = ode15s(@fun_NonInf_SepM, report_time, init, options,...
                k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,epsilon);
            
%% Some plots 1
figure(2)
plot(report_time, y(:,1),'k');
hold on 
plot(report_time, y(:,2),'g');
plot(report_time, y(:,3),'r');
hold off 
legend('M0','M1','M2')


%% 
%{
s1 = 0;
s2 = 0;
[~,y1] = ode15s(@fun, report_time, init, options,...
                k1,k2,k_m1,k_m2,delta,s1,s2,M0,g);
            
figure(2)
plot(report_time, y1(:,1),'k');
hold on 
plot(report_time, y1(:,2),'g');
plot(report_time, y1(:,3),'r');
hold off 

legend('M0','M1','M2')
%}
%% 


s1 = 1;
s2 = 1;
[~,y2] = ode15s(@fun_NonInf_SepM, report_time, init, options,...
                k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,epsilon);
%           
figure(1)
plot(report_time, y2(:,1),'k','LineWidth',2);
hold on 
plot(report_time, y2(:,2),'k-.','LineWidth',2);
plot(report_time, y2(:,3),'k--','LineWidth',2);
%hold off 
xlabel('Time (days)')
ylabel('Number of macrophages (cells)')
%title('Macrophages in healthy condition')
legend('M','M1','M2','FontSize',20)
set(gca, 'FontSize',24)
%}
%% M1-M2 Interation


k12 = 0.09;
k21 = 0.02; %0.02
[~,y2] = ode15s(@funInterM, report_time, init, options,...
                k1,k2,k_m1,k_m2,delta,k12,k21,M0,g,epsilon);
            
figure(3)
plot(report_time, y2(:,1),'k','LineWidth',2);
hold on 
plot(report_time, y2(:,2),'k-.','LineWidth',2);
plot(report_time, y2(:,3),'k--','LineWidth',2);
hold off 
xlabel('Time (days)')
ylabel('Number of macrophages (cells)')
%title('Macrophages in healthy condition')
legend('M0','M1','M2','FontSize',20)
set(gca, 'FontSize',24)

%% Immunomodulators 
epsilon = 0;
lambda = log(2) / (.5 - 0); % 

t_im_0 = 0;
t_im_end = 15;
report_point_im = (t_im_end - t_im_0) * 24 + 1;
report_time_im = linspace(t_im_0,t_im_end, report_point_im);
len_im = length(report_time_im);

%%
M_im = zeros(1,len_im);
M1_im = zeros(1,len_im);
M2_im = zeros(1,len_im);
Eff = zeros(1,len);


M_im0 = y2(end,1);
M1_im0 = y2(end,2);
M2_im0 = y2(end,3);
Eff_0 = 1;

M_im(1) = M_im0;
M1_im(1) = M1_im0;
M2_im(1) = M2_im0;
Eff(1) = Eff_0;

init_im = [M_im0, M1_im0, M2_im0,Eff_0]';

%%
[~,y_im] = ode15s(@fun, report_time_im, init_im, options,...
                k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,epsilon,lambda);


%% Plot M1 time-series 
figure(35)
plot(report_time_im, y_im(:,2),'k','LineWidth',1.5)
hold on 
yline(y2(end,2),'k--','LineWidth',1.5);
hold off 
%xticklabels({'0','1','2','3','4','5','6','7'});
%xticks(1:1:7);
legend('administration', 'Non-administration')
xlabel('Days post administration')
ylabel('Number of M1 cells')
set(gca, 'FontSize',22)
%% Immunomodulators 2
epsilon = 0;

t_im2_0 = 1001;
t_im2_end = 1007;
report_point_im2 = (t_im2_end - t_im2_0) * 24 + 1;
report_time_im2 = linspace(t_im2_0,t_im2_end, report_point_im2);
len_im2 = length(report_time_im2);

%%
M_im2 = zeros(1,len_im2);
M1_im2 = zeros(1,len_im2);
M2_im2 = zeros(1,len_im2);

M_im20 = y_im(end,1);
M1_im20 = y_im(end,2);
M2_im20 = y_im(end,3);

M_im2(1) = M_im20;
M1_im2(1) = M1_im20;
M2_im2(1) = M2_im20;

init_im2 = [M_im20, M1_im20, M2_im20]';

%%
[~,y_im2] = ode15s(@fun, report_time_im2, init_im2, options,...
                k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,epsilon);







%%
%%%%%%% TIV - M1MM2 %%%%%%%

%% Viral parameters 

beta = 3.8e-5; %3.8e-5 (benchmark) %2.244e-5 R0 = 1.
delta_I = 3.6;
p = 7.1e-2; % 5e-2, 11e-2, 15e-2
c = 20; %%change to 20 Cao et al
kappa = 24 * 3.2e-5;
q1 = 1e-6;%1e-6;  % cells 
q2 = 1e-6;%1e-6; % virus

kappa_a = 0.2;
mu =  1e-6; %1e-6; %2.5e-6;
rho = 1; %1

%epsilon = 0;

%% time setting 

t0_inf = 0;
t_end_inf = 15;
report_point_inf = (t_end_inf - t0_inf) * 24 + 1;
report_time_inf = linspace(t0_inf, t_end_inf, report_point_inf);
len_inf = length(report_time_inf);

%% Viral Variables 


M_inf = zeros(1,len_inf);
M1_inf = zeros(1,len_inf);
M2_inf = zeros(1,len_inf);
T = zeros(1,len_inf);
I = zeros(1,len_inf);
V = zeros(1,len_inf);
A = zeros(1,len_inf);
M1_inf_1 = zeros(1,len_inf);
M1_inf_2 = zeros(1,len_inf);
M1_inf_3 = zeros(1,len_inf);
Eff_inf = zeros(1,len);



M_inf0 = y_im(end,1);  % y_im(end,1)
M1_inf0 = y_im(end,2); % y_im(end,2)
M2_inf0 = y_im(end,3); % y_im(end,3)


T0 = 4e+8;
I0 = 0;
V0 = 3.3e-1; % 3.3e-1
A0 = 0;
M1_inf_10 = y_im(end,2);
M1_inf_20 = 0;
M1_inf_30 = 0;
Eff_inf0 = y_im(end,4);

M_inf(1) = M_inf0;
M1_inf(1) = M1_inf0;
M2_inf(1) = M2_inf0;
T(1) = T0;
I(1) = I0;
V(1) = V0;
A(1) = A0;
M1_inf_1(1) = M1_inf_10;
M1_inf_2(1) = M1_inf_20;
M1_inf_3(1) = M1_inf_30;
Eff_inf(1) = Eff_inf0;


init_inf = [M_inf0,M1_inf0,M2_inf0,T0,I0,V0,A0,M1_inf_10,M1_inf_20,M1_inf_30,Eff_inf0]';




%%  ode15s
%h = waitbar(0,'Please wait...');
%for i = 1:1000
[~,y_inf] = ode15s(@fun_inf_lambda, report_time_inf, init_inf, options,...
                        k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,...
                        beta,delta_I,p,c,kappa,q1,q2,...
                        kappa_a,mu,rho, epsilon,lambda);
%     waitbar(i/1000,h);
%end 
%close(h)       

%%

%C1 = delta_I * y_inf(1,2) * (k_m1 + delta) / (y_inf(1,6) * y_inf(1,1) * q1 * T0);
%C2 = delta_I * k1/(1 + s1*(y_inf(1,3)/M0)) / (y_inf(1,6) * q1 * T0);
%C3 = delta_I / T0;

C1 = y_inf(1,2)/y_inf(1,1) * (k_m1 + delta);
C2 =  k1/(1 + s1*(y_inf(1,3)/M0));

C3 = q1 * y_inf(1,5) + q2 * y_inf(1,6);



C4 = (k1/(1 + s1*(y_inf(1,3)/M0)) + q1 * y_inf(1,5) + q2 * y_inf(1,6)) * y_inf(1,1);
C5 = (delta + k_m1) * y_inf(1,2);

%%
%{
beta = 1e-5:2e-6:1.2e-4;
len_beta = length(beta);
l1 = zeros(1,len_beta);
for j = 1:len_beta 
    [~,y_inf] = ode15s(@fun_inf_lambda, report_time_inf, init_inf, options,...
                        k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,...
                        beta(j),delta_I,p,c,kappa,q1,q2,...
                        kappa_a,mu,rho, epsilon,lambda);
     l1(j) = (beta(j) * p *q1 * y_inf(1,4)* max(y_inf(:,5)) * y_inf(1,1))/((k_m1 + delta)*y_inf(1,2)*delta_I * (c + kappa * y_inf(1,2) + kappa_a * y_inf(1,7)));%using I
end
%}
%%
beta = 1e-5:2e-6:1.2e-4;
len_beta = length(beta);
e3 = zeros(1,len_beta);
%RV = zeros(1,len_beta);
for i = 1:len_beta 
%for j = 1:len_inf

      [~,y_inf] = ode15s(@fun_inf_lambda, report_time_inf, init_inf, options,...
                        k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,...
                        beta(i),delta_I,p,c,kappa,q1,q2,...
                        kappa_a,mu,rho, epsilon,lambda);
                    
    e3(i) = max((k1./(1 + s1*(y_inf(:,3)/M0))+ q1 * y_inf(:,5) + q2 * y_inf(:,6)) .* y_inf(:,1) ./((k_m1 + delta) * y_inf(:,2)));
                
    %e(j) = (q1 * y_inf(j,5) + q2 * y_inf(j,6)) / (k_m1 + delta);
    %e2(j) = (y_inf(j,6) * y_inf(j,4) * q1 * beta)/ ((k_m1 + delta) * delta_I); %using V
    %e3(i) = max(((beta(i) * p *q1 .* y_inf(:,4) .* y_inf(:,5) +q2./(beta(i) * y_inf(:,4))+ k1./(1 + s1*(y_inf(:,3)/M0))) .* y_inf(:,1))./((k_m1 + delta).*y_inf(:,2).*delta_I .* (c + kappa .* y_inf(:,2) + kappa_a .* y_inf(:,7))));%using I
    %RV(i) = max(p * beta(i) .* y_inf(:,4) ./ (delta_I .* (c + kappa .* y_inf(:,2) + kappa_a .* y_inf(:,7))));
end 

%%
figure(3)
scatter(beta,e3,'o',...
                    'MarkerFaceColor',[0.4940 0.1840 0.5560],...
                    'LineWidth',.8, 'MarkerEdgeAlpha',.5, 'MarkerFaceAlpha',.5, 'MarkerEdgeColor', 'none')
xlabel('\beta')
ylabel('Max R_{M1}')
set(gca,'FontSize',22)
%%

xlabel('\beta')
ylabel('R_{M1}')
legend('High \beta','Medium \beta','Low \beta')
set(gca,'FontSize',22)


%% 
%In this section, the Jacobin matrix of the three macrophage populations
%are calculated. 

Ja_Mat = zeros(3,3);
E = zeros(1,len_inf);



for j = 1:len_inf

Ja_Mat(1,1) = g - (2*g*y_inf(j,1)/M0 - g*y_inf(j,2)/M0 - g*y_inf(j,3)/M0) - k1/(1 + s1*(y_inf(j,3)/M0)) - k2 * (1 + s2* (y_inf(j,2)/M0)) - q1 * y_inf(j,5) - q2 * y_inf(j,6);
Ja_Mat(1,2) = -g*y_inf(j,1)/M0 + k_m1 - k2*s2*y_inf(j,1)/M0;
Ja_Mat(1,3) = -g*y_inf(j,1)/M0 + k_m2 + k1*s1*y_inf(j,1)/(M0*(1+s1*y_inf(j,3)/M0)^2);

Ja_Mat(2,1) = k1/(1 + s1*(y_inf(j,3)/M0)) + q1 * y_inf(j,5) + q2 * y_inf(j,6);
Ja_Mat(2,2) = -(k_m1 + delta);
Ja_Mat(2,3) = - k1*s1*y_inf(j,1)/(M0*(1+s1*y_inf(j,3)/M0)^2);

Ja_Mat(3,1) = k2 * (1 + s2* (y_inf(j,2)/M0));
Ja_Mat(3,2) = k2*s2*y_inf(j,1)/M0;
Ja_Mat(3,3) = -(k_m2 + delta);




E(j)  = max(eig(Ja_Mat));
end 

%%
plot(E)

%% 
% Next generation method to find the basic reproduction number, assuming M
% and M1 are infecitous components. 

FF = zeros(2,2);
VV = zeros(2,2);
E2 = zeros(1,len_inf);

for j = 1:len_inf
FF(1,1) = g - (2*g*y_inf(j,1)/M0 - g*y_inf(j,2)/M0 - g*y_inf(j,3)/M0);
FF(1,2) = k_m1 - g*y_inf(j,1)/M0;
FF(2,1) = k1/(1 + s1*(y_inf(j,3)/M0)) + q1 * y_inf(j,5) + q2 * y_inf(j,6);
FF(2,2) = 0;

VV(1,1) = k1/(1 + s1*(y_inf(j,3)/M0)) + k2 * (1 + s2* (y_inf(j,2)/M0))+ q1 * y_inf(j,5) + q2 * y_inf(j,6);
VV(1,2) = k2 * s2 * y_inf(j,1)/M0;
VV(2,1) = 0;
VV(2,2) = delta + k_m1;

Operator = FF * inv(VV);
E2(j) = max(eig(Operator));
end 
%%
plot(E2)
hold on 

%% 
% M1 reproduction number (Normalized) for a single value of beta.
beta = 1e-5:2e-6:2.8e-5;%1.2e-4;
len_beta = length(beta);
R_values = zeros(len_beta,3);
%MM1 = zeros(1,len_inf);
R_m1_vector = zeros(len_beta,len_inf);
R_m1_vector_round = zeros(len_beta,len_inf);
for i = 1:len_beta 
        [~,y_inf] = ode15s(@fun_inf_lambda, report_time_inf, init_inf, options,...
                        k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,...
                        beta(i),delta_I,p,c,kappa,q1,q2,...
                        kappa_a,mu,rho, epsilon,lambda);
    for j = 1:len_inf
        R_m1_vector(i,j) = (k1/(1 + s1*(y_inf(j,3)/M0))+ q1 * y_inf(j,5) + q2 * y_inf(j,6)) * y_inf(j,1) /((k_m1 + delta) * y_inf(j,2));
        R_m1_vector_round(i,j) = round(R_m1_vector(i,j),2);
    end 
        indx = find(R_m1_vector_round(i,:) ~= 1);
        if isempty(indx) == 1
            R_values(i,2) = 15;
        else 
            R_values(i,2) = (indx(1)-1)/24;
        end 
        R_values(i,1) = beta(i);
        R_values(i,3) = max(R_m1_vector(i,:));
end 



beta_II = 3e-5:2e-6:6.08e-5;%1.2e-4;
len_beta_II = length(beta_II);
R_values_II = zeros(len_beta_II,3);
R_m1_vector_II = zeros(len_beta_II,len_inf);
R_m1_vector_round_II = zeros(len_beta,len_inf);

for i = 1:len_beta_II 
        [~,y_inf] = ode15s(@fun_inf_lambda, report_time_inf, init_inf, options,...
                        k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,...
                        beta_II(i),delta_I,p,c,kappa,q1,q2,...
                        kappa_a,mu,rho, epsilon,lambda);
    for j = 1:len_inf
        R_m1_vector_II(i,j) = (k1/(1 + s1*(y_inf(j,3)/M0))+ q1 * y_inf(j,5) + q2 * y_inf(j,6)) * y_inf(j,1) /((k_m1 + delta) * y_inf(j,2));
        R_m1_vector_round_II(i,j) = round(R_m1_vector_II(i,j),2);
    end 
        indx_II = find(R_m1_vector_round_II(i,:) ~= 1);
        if isempty(indx_II) == 1
            R_values_II(i,2) = 15;
        else 
            R_values_II(i,2) = (indx_II(1)-1)/24;
        end 
        R_values_II(i,1) = beta_II(i);
        R_values_II(i,3) = max(R_m1_vector_II(i,:));
end 







beta_III = 6.1e-5:2e-6:1.2e-4;
len_beta_III = length(beta_III);
R_values_III = zeros(len_beta_III,3);
R_m1_vector_III = zeros(len_beta_III,len_inf);
R_m1_vector_round_III = zeros(len_beta,len_inf);

for i = 1:len_beta_III 
        [~,y_inf] = ode15s(@fun_inf_lambda, report_time_inf, init_inf, options,...
                        k1,k2,k_m1,k_m2,delta,s1,s2,M0,g,...
                        beta_III(i),delta_I,p,c,kappa,q1,q2,...
                        kappa_a,mu,rho, epsilon,lambda);
    for j = 1:len_inf
        R_m1_vector_III(i,j) = (k1/(1 + s1*(y_inf(j,3)/M0))+ q1 * y_inf(j,5) + q2 * y_inf(j,6)) * y_inf(j,1) /((k_m1 + delta) * y_inf(j,2));
        R_m1_vector_round_III(i,j) = round(R_m1_vector_III(i,j),2);
    end 
        indx_III = find(R_m1_vector_round_III(i,:) ~= 1);
        if isempty(indx_III) == 1
            R_values_III(i,2) = 15;
        else 
            R_values_III(i,2) = (indx_III(1)-1)/24;
        end 
        R_values_III(i,1) = beta_III(i);
        R_values_III(i,3) = max(R_m1_vector_III(i,:));
end 





%%

scatter(beta,R_values(:,2),'o',...
             'MarkerFaceColor',[0.3010 0.7450 0.9330],...
             'LineWidth',.8, 'MarkerEdgeAlpha',.8, 'MarkerFaceAlpha',.8, 'MarkerEdgeColor', 'none','SizeData',15*fliplr(R_values(:,3)))

hold on 
scatter(beta_II,R_values_II(:,2),'o',...
             'MarkerFaceColor',[0.4940 0.1840 0.5560],...
             'LineWidth',.8, 'MarkerEdgeAlpha',.8, 'MarkerFaceAlpha',.8, 'MarkerEdgeColor', 'none','SizeData',15*fliplr(R_values_II(:,3)))

scatter(beta_III,R_values_III(:,2),'o',...
             'MarkerFaceColor',[0.9290 0.6940 0.1250],...
             'LineWidth',.8, 'MarkerEdgeAlpha',.8, 'MarkerFaceAlpha',.8, 'MarkerEdgeColor', 'none','SizeData',15*fliplr(R_values_III(:,3)))
 
plot([beta,beta_II,beta_III],[R_values(:,2)',R_values_II(:,2)',R_values_III(:,2)'] ,'k:','LineWidth',.5)   
  

xlabel('\beta') 
ylabel('Days when R_{M1} > 1')
%legend('Natural regulation', 'Activation by infection', 'Overall')
legend('Region-I','Region-II','Region-III','EdgeColor','None')
set(gca, 'FontSize',24)         



axes('position',[0.178571428571429,0.352380952380952,0.271428571428571,0.247619047619049]) % create a new pair of axes inside current figure
box on % put box around new pair of axes


plot(report_time_inf, R_m1_vector(1,:),'Color',[0.3010 0.7450 0.9330],'LineWidth',1.5)
hold on 
scatter(R_values(1,2),1,'o',...
             'MarkerFaceColor',[0.3010 0.7450 0.9330],...
             'LineWidth',.8, 'MarkerEdgeAlpha',.8, 'MarkerFaceAlpha',.8, 'MarkerEdgeColor', 'none');
hold on 
plot(report_time_inf,R_m1_vector_II(5,:),'Color',[0.4940 0.1840 0.5560],'LineWidth',1.5)
scatter(R_values_II(5,2),1,'o',...
             'MarkerFaceColor',[0.4940 0.1840 0.5560],...
             'LineWidth',.8, 'MarkerEdgeAlpha',.8, 'MarkerFaceAlpha',.8, 'MarkerEdgeColor', 'none');
hold on 
plot(report_time_inf,R_m1_vector_III(1,:),'Color',[0.9290 0.6940 0.1250],'LineWidth',1.5)

scatter(R_values_III(1,2),1,'o',...
             'MarkerFaceColor',[0.9290 0.6940 0.1250],...
             'LineWidth',.8, 'MarkerEdgeAlpha',.8, 'MarkerFaceAlpha',.8, 'MarkerEdgeColor', 'none');

         
xlabel('Days post infection (p.i.)')
ylabel('R_{M_1}')
%axis tight
set(gca,'FontSize',22)


%% Calculation of the basic reproduction number 
beta_R = p*beta.*y_inf(:,4)./(delta_I .* (c + kappa.* y_inf(:,2) + kappa_a.*y_inf(:,7)));
plot(report_time_inf, beta_R,'k','LineWidth',1.5)
hold on 
xlabel('Days post infection (p.i.)')
ylabel('The basic reproduction number')
legend('\beta = 2.88x10^{-5}','\beta = 6.08x10^{-5}','\beta = 10.08x10^{-5}')
set(gca, 'FontSize',22)

%% Plot M1 changes post viral challenge
figure(67)
plot(report_time_inf, y_inf(:,2), 'k:', 'LineWidth',1.5)
hold on 
%plot(report_time_inf, y_inf(:,3), 'r', 'LineWidth',1.5)
xlabel('Days post infection (p.i.)')
ylabel('Number of macrophages')
%legend('benchmark','1 day','3 days','7 days')
%legend('q_1 = 1x10^{-5}','q_1 = 1x10^{-6}', 'q_1 = 1x10^{-7}')
legend('k_{-1} = 0.65', 'k_{-1} = 0.33', 'k_{-1} = 0.1','k_{-1} = 0.05')
set(gca,'FontSize',22)

%% 
%AUC_M1 = zeros(1,4);
AUC_M1(4) = trapz(y_inf(:,2));

